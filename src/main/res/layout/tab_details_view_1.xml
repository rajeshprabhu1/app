<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/LayoutMatch"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/activity_par_lay"
        style="@style/LayoutWidMatch_HeiWei">

        <RelativeLayout
            android:id="@+id/activity_inner_lay"
            style="@style/LayoutWidMatch_HeiWrap"
            android:layout_height="@dimen/size35"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/size100"
                android:layout_height="@dimen/size20"
                android:layout_centerVertical="true"
                android:src="@drawable/place_type_discovered"
                />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/details_par_lay"
        style="@style/LayoutWidMatch_HeiWei">
        <RelativeLayout
            android:id="@+id/details_inner_lay"
            style="@style/LayoutWidMatch_HeiWrap"
            android:layout_height="@dimen/size35"
            android:gravity="center">
            <ImageView
                android:layout_width="@dimen/size130"
                android:layout_height="@dimen/size20"
                android:layout_centerVertical="true"
                android:src="@drawable/place_type_un_discovered"
                />
        </RelativeLayout>
    </RelativeLayout>

